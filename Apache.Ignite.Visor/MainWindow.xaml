<Window x:Class="Apache.Ignite.Visor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:model="clr-namespace:Apache.Ignite.Visor.Model"
        mc:Ignorable="d"
        Title="Ignite.NET Cluster Monitor" Height="350" Width="525" d:DataContext="{d:DesignInstance model:Cluster}">
    <TabControl>
        <TabItem Header="Cluster">
            <GroupBox Header="Nodes">
                <DataGrid ItemsSource="{Binding Nodes}" />
            </GroupBox>
        </TabItem>
        <TabItem Header="Caches">
            
        </TabItem>
    </TabControl>
</Window>
